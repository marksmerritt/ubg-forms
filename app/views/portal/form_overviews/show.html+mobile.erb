<div class="container">
  <div class="row">
    <div class="col-md-12">
      <h3>Choose a Form</h3>  
    </div>
  </div>

  <% @form_types.each do |form_type| %>
    <div class="row">
      <div class="col-md-12">
        <%= link_to "#{form_type.name}", new_form_type_form_path(form_type), id: "create-new-form-#{form_type.id}", class: "new-form-link" %>
      </div>
    </div>
  <% end %>
</div>
